        }
        else {
            draw_set_color(c_black);
        }
           
        stringWidth = string_width(menu[i]);
        stringHeight = string_height(menu[i]);
        centerStringPosX = (winWidth) / 2;
        centerStringPosY = verticalButtonSpace * (i-1) + firstButtonYPos - stringHeight / 2;
        draw_text( centerStringPosX, centerStringPosY, menu[i] );
    }
    
}
